<!-- Add the relhax gui sound banks to audio_mods.xml -->
<patch>
  <type>xml</type>
  <mode>add</mode>
  <file>\\res_mods\\versiondir\\audioww\\audio_mods.xml</file>
  <path>//audio_mods.xml/loadBanks</path>
  <search></search>
  <replace>bank/RelHaxGui.bnk</replace>
</patch>
